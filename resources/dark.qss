        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.5 #474c50,
        stop:1 #393e41
    );
	border: 1px outset #282b2e;
	border-radius: 8px;
	padding: 0px 5px 0 5px;
}
QHeaderView::section {
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.5 #474c50,
        stop:1 #393e41
    );
	border: 1px outset #282b2e;
	border-radius: 8px;
