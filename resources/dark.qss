        x2:0, y2:1,
        stop:0 #c8ced2,
        stop:1 #a1a6aa
    );
 }

/*QMenu*/
QMenu {
	font-size: 11px;
	border: 1px outset #282b2e;
    border-radius: 6px;
	margin: 2px;
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.1 #474c50,
        stop:0.9 #474c50,
        stop:1 #393e41
    );
 }
 QMenu::item {
    border-radius: 6px;
    padding: 5px;
    border: 1px solid transparent;
	background-color: transparent;
 }
 QMenu::item:selected {
    border-radius: 6px;
	border: 1px outset #282b2e;
	background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #c8ced2,
        stop:0.5 #a1a6aa,
        stop:1 #7f8387
    );
 }
 QMenu::icon:checked {
     background: gray;
     border: 1px inset gray;
     position: absolute;
     top: 1px;
     right: 1px;
     bottom: 1px;
     left: 1px;
 }
 QMenu::separator {
     height: 1px;
     background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #171a1b,
        stop:0.2 #272a2c,
        stop:1 #232528
    );
     margin-left: 5px;
     margin-right: 5px;
 }
 QMenu::indicator {
     width: 13px;
     height: 13px;
 }

/*QHeaderView*/
QHeaderView::section {
