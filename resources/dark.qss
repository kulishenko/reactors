/*QWidget*/
QWidget {
    background-color: transparent;
	color: #ffffff;
	font-family: Arial;
	font-size: 12px;
	font-weight:bold;
}

/*QPushButton*/
QPushButton {
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #4d93dd,
        stop:0.5 #326fae,
        stop:1 #225a89
    );
	border: 1px outset #282b2e;
	border-radius: 8px;
	padding: 5px;
}
QPushButton:pressed {
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #1a1d1e,
        stop:0.15 #2a2d30,
        stop:1 #2d3033
    );
 }
QPushButton:disabled {
	color: #888888;
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.5 #474c50,
        stop:1 #393e41
    );
 }

/*QToolButton*/
QToolButton {
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #4d93dd,
        stop:0.5 #326fae,
        stop:1 #225a89
    );
	border: 1px outset #282b2e;
	border-radius: 8px;
	padding: 5px;
}
QToolButton:pressed {
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #1a1d1e,
        stop:0.15 #2a2d30,
        stop:1 #2d3033
    );
 }

/*QCheckBox*/
QCheckBox {
	spacing: 5px;
 }
 QCheckBox::indicator {
    width: 18px;
    height: 18px;
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #1a1d1e,
        stop:0.15 #2a2d30,
        stop:1 #2d3033
    );
	border: none;
	border-radius: 5px;
 }
QCheckBox::indicator:unchecked:hover {
 background-color:QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #171a1b,
        stop:0.2 #272a2c,
        stop:1 #2a2c20
    );
 }
QCheckBox::indicator:unchecked:pressed {
 background-color:QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #171a1b,
        stop:0.2 #272a2c,
        stop:1 #2a2c20
    );
 }
QCheckBox::indicator:checked {
     image: url(:/images/images/checkbox_checked.png);
 }
QCheckBox::indicator:checked:hover {
 background-color:QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #171a1b,
        stop:0.2 #272a2c,
        stop:1 #2a2c20
    );
 }
QCheckBox::indicator:checked:pressed {
 background-color:QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #171a1b,
        stop:0.2 #272a2c,
        stop:1 #2a2c20
    );
 }

