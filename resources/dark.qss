/*QGroupBox*/
QGroupBox {
     background-color: #313538;
	 border: 1px outset #282b2e;
     border-radius: 8px;
     margin-top: 8px;
 }
 QGroupBox::title {
     subcontrol-origin: margin;
     subcontrol-position: top center;
     padding: 0 3px;
 }

/*QProgressBar*/
QProgressBar {
     text-align: center;
	 border: 1px outset #282b2e;
     border-radius: 8px;
     background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #171a1b,
        stop:0.2 #272a2c,
        stop:1 #232528
    );
 }
 QProgressBar::chunk {
    border-radius: 8px;
 	background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #4d93dd,
        stop:0.5 #326fae,
        stop:1 #225a89
    );
 }

/*QLabel*/
QLabel {
	 border: 1px outset #282b2e;
     border-radius: 8px;
	 padding: 5px;
     background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.1 #474c50,
        stop:0.9 #474c50,
        stop:1 #393e41
    );
 }

/*QStatusBar*/
QStatusBar {
	 font-size: 11px;
	 border: 1px outset #282b2e;
     border-radius: 6px;
	 margin: 2px;
	 padding: 3px;
     background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.1 #474c50,
        stop:0.9 #474c50,
        stop:1 #393e41
    );
 }

/*QMenuBar*/
QMenuBar {
	 font-size: 11px;
	 border: 1px outset #282b2e;
