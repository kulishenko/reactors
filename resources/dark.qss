/*QTabWidget*/
QTabWidget::pane { /* The tab widget frame */
     border-top: 1px solid #272a2c;
     position: absolute;
     top: -0.75em;
 }
 QTabWidget::tab-bar {
     alignment: center;
 }
 QTabBar::tab {
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.5 #474c50,
        stop:1 #393e41
    );
	border-radius: 8px;
    min-width: 8ex;
    padding: 5px;
	margin: 0px 2px 0px 2px;
 }
 QTabBar::tab:selected, QTabBar::tab:hover {
     background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #171a1b,
        stop:0.2 #272a2c,
        stop:1 #232528
    );
 }
 QTabBar::tab:selected {
     border-color: #9B9B9B;
     border-bottom-color: #C2C7CB; 
 }

/*QToolBox*/
QToolBox::tab {
     background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #585e62,
        stop:0.5 #474c50,
        stop:1 #393e41
    );
     border-radius: 5px;
     color: darkgray;
 }
 QToolBox::tab:selected {
     color: white;
 }
 QToolBox::tab:hover {
